{
    "name": "emkcloud/obsidian-ui",
    "description": "Emkcloud Obsidian Package UI",
    "keywords": [
        "ui",
        "components",
        "laravel",
        "livewire"
    ],
    "homepage": "https://github.com/emkcloud/obsidian-ui",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimo Della Rovere",
            "email": "dellarovere@duck.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Emkcloud\\ObsidianUI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "livewire/livewire": "^3.5.19"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emkcloud\\ObsidianUI\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
